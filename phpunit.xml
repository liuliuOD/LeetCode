<?xml version="1.0" encoding="UTF-8"?>
<phpunit colors="true">
    <testsuites>
        <testsuite name="Hacker Rank">
            <directory>Tests</directory>
        </testsuite>
    </testsuites>
    <logging>
        <log type="coverage-clover" target="coverage-clover/logs/clover.xml"/>
    </logging>
    <filter>
        <whitelist processUncoveredFilesFromWhitelist="true">
            <directory suffix=".php">Resolved</directory>
        </whitelist>
    </filter>
</phpunit>